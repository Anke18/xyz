          "geomj": "ST_AsGeoJson(geom)",
          "geom": "geom",
          "arrayZoom": {
            "2": "store_survey"
          },
          "style": {
            "stroke": false,
            "color": "#555",
            "fillOpacity": 1
          },
          "styleHighlight": {
            "stroke": false,
            "color": "#555",
