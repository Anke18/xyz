          "dbs": "XYZ",
          "qID": "qID",
          "infoj": [
              {
                  "field": "retailer",
                  "label": "Retailer",
                  "type": "text"
              },
              {
                  "field": "fascia",
                  "label": "Fascia",
                  "type": "text"
              },
              {
                  "field": "fascia_site_visit",
                  "label": "Fascia during site visit",
                  "type": "text"
              },
              {
                  "field": "store_pitch",
                  "label": "Store pitch",
                  "type": "text",
                  "options": ["undefined", "Very Good", "Good", "Average", "Poor", "Terrible"]
              },
              {
                  "field": "store_size_sqm",
                  "label": "Store size sqm",
                  "type": "text",
